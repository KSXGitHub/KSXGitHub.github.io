<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <meta charset="UTF-8" />
    <title>Prime Generator</title>
    <link rel="stylesheet" href="style.css" />
  </head>
  <body>
    <div>
      <span>Number of primes: </span>
      <input id="input" type="number" placeholder="positive integer" />
      <button id="addprime">Add to list</button>
      <button id="reset">Reset list</button>
    </div>
    <div>
      <a id="gettxt" class="hidden" href="#" download="primes.txt">Get txt file</a>
    </div>
    <div id="time"></div>
    <div id="output">2</div>
    <script type="text/javascript" src="prime.js" />
    <script type="text/javascript" src="main.js" />
  </body>
</html>
